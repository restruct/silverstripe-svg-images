{
  "name": "restruct/silverstripe-svg-images",
  "description": "Basic SVG Image support for Silverstripe (Assets/Uploads)",
  "type": "silverstripe-vendormodule",
  "license": "BSD-3-Clause",
  "keywords": [
    "silverstripe"
  ],
  "homepage": "https://github.com/restruct/silverstripe-svg-images",
  "authors": [
    {
      "name": "Restruct web & apps",
      "email": "dev@restruct.nl"
    }
  ],
  "require": {
    "silverstripe/cms": "^4 || ^5",
    "contao/imagine-svg": "^1.0",
    "enshrined/svg-sanitize": "^0.20"
  },
  "suggest": {
    "ext-gd": "Required for generating PNG test images in /dev/svg-compare"
  },
  "autoload": {
    "psr-4": {
      "Restruct\\Silverstripe\\SVG\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "silverstripe/vendor-plugin": true,
      "silverstripe/recipe-plugin": true
    }
  }
}
