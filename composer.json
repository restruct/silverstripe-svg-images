{
  "name": "restruct/silverstripe-svg-images",
  "description": "SVG Image support for Silverstripe with real manipulation and sanitization",
  "type": "silverstripe-vendormodule",
  "license": "MIT",
  "keywords": [
    "silverstripe",
    "svg"
  ],
  "homepage": "https://github.com/restruct/silverstripe-svg-images",
  "authors": [
    {
      "name": "Restruct web & apps",
      "email": "dev@restruct.nl"
    }
  ],
  "require": {
    "silverstripe/cms": "^6",
    "contao/imagine-svg": "^1.0",
    "enshrined/svg-sanitize": "^0.20",
    "ext-dom": "*"
  },
  "suggest": {
    "ext-gd": "Required for generating PNG test images in /dev/svg-compare"
  },
  "autoload": {
    "psr-4": {
      "Restruct\\Silverstripe\\SVG\\": "src/"
    }
  }
}
